 .eic-presence .msg {
    color:rgb(0, 151, 0);
    text-align: center;
}
.eic-presence .msg.error {
    color:rgb(223, 5, 5);
}
.infaTileContent.eic-presence .form {
    padding-top: 14px;
    padding-bottom: 24px;
    margin: auto;
    width: 310px;
}
.login-page .infaTileContent.eic-presence .form {
    margin: auto 60px auto;
}

.infaTileContent.eic-presence .form .infaTextBox{
    float: right;
    width: 210px;
    margin-bottom: 10px;
}

.infaTileContent.eic-presence .form .flabel {
    height: 24px;
    margin-bottom: 0px;
    padding-top: 5px;
    text-align: left;
    clear: both;
    float: left;
}

.infaTileContent.eic-presence .field-label1 {
    font: 500 1em "Roboto",Arial,Helvetica,sans-serif;
}
.infaTileContent.eic-presence .footer{
    width: 100%;
    float: left;
    clear: both;
   
}
.login-page .infaTileContent.eic-presence .footer{
    margin-top:50px;
}
.infaTileContent.eic-presence .form .submitBtn {
    float:left;
    text-align: center;
    background: orange;
    height: 40px;
    text-align: center;
    line-height: 40px;
    font-weight: bold;
    color: white;
    cursor: pointer;    
    font-size: 18px;
    width: 100%;
    
}
.infaTileContent.eic-presence .form .multibuttons .submitBtn{
    width: 48%;
    margin: 3px;
}

.infaTileContent.eic-presence .login-container .infaTileHeader {
    height: 50px;
}

.infaTileContent.eic-presence input.infaField{
    width: 100%;
    height: 25px;
}

.infaTileContent.eic-presence #namespace {
    padding-top: 2px;
    width: 65%;
    padding-left: 10px;
    float: left;
}
.login-page .infaTileContent.eic-presence #namespace {
    width: 100%;
    padding-left: 0;
}
.infaTileContent.eic-presence #namespace select {
   height: 28px;
   width: 214px
}
.login-page .infaTileContent.eic-presence #namespace select {
    width: 100%;
}
.infaTileContent.eic-presence .rememberMe{
    float: left;
    padding-bottom: 10px;
}

.infaTileContent.eic-presence input.infaField{
    margin-bottom: 10px;
}