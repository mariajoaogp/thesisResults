.eic-presence.tilePage {
    background-repeat: no-repeat;
    background-position: center center;
    background-attachment: fixed;
    background-size: cover;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    overflow: auto;
    background-color: black;
    display: table;
    visibility: visible;
    width: 100%;
    height: 100%;
}
.login-page .eic-presence.tilePage {
    background: none;
}
.extensionSlidingWindow .tilePage::before{
    height: 30%;
}
.ext-body .loginDiv{
    display: none;
}
.eic-presence.tilePage:before {
    content: '';
    display: table-row;
    vertical-align: middle;
}

.eic-presence.tilePage .tileContainer {
    display: table;
    margin: 0 auto;
    vertical-align: middle;
    font: 500 1em "Roboto", Arial, Helvetica, sans-serif;    
}

.eic-presence .login-container{
    background-color: #fff;
    text-align: initial;
    border-radius: 0px 0px 4px 4px;
    width: 450px;
    /* min-height: 200px; */
    position: relative;
    min-height: 392px;
    /* display: inline-block; */
    vertical-align: middle;
    border-radius: 5px;
}

.eic-presence .login-container .infaTileHeader {
    height: 50px;
}

.eic-presence .product-logo{
    height: 100px;    
}

.eic-presence .login-container .infaTileHeader .product-logo {
    width: 100%;
    float: left;
    margin-top: 10px;
    margin-left: 10px;
}
.eic-presence .product-logo > div{
    float: right;
    width: 81%;
}
.eic-presence .product-logo .title{
    margin-top: 10px;
    font-weight: bold;
    font-size: 14px;
}

.eic-presence .product-logo img {
    height: auto;
    width: 50px;
}

.eic-presence .product-name {
    padding-top: 17px;
    box-sizing: border-box;
    float: left;
}

/* div.infaTileContent.eic-presence{
