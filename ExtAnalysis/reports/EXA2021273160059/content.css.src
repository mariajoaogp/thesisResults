  height: 40px;
  float: left;
  background: #333333;
}

.getdata-sidebar #columns .column .counter {
  width: 40px !important;  
  cursor: pointer;
  height: 40px;
  margin: 0px;
  display: inline-block;

  font-family: "Open Sans";
  font-style: normal;
  font-weight: 600;
  font-size: 11px;
  line-height: 40px;
  /* identical to box height */

  display: block;
  align-items: center;
  text-align: center;
  /*box-shadow: 1px 0px 0px rgba(255, 255, 255, 0.1);  */
  color: #8D8D8D;  
  background: #363535;
  border: 1px solid #222222;

}

.getdata-sidebar #columns .column.active .counter {
  /*box-shadow: 1px 0px 0px rgba(255, 255, 255, 0.1), 0px -1px 0px #FFFFFF;*/
  color: #FFFFFF;  
  background: #222222;
}



.getdata-sidebar #columns .column .col-name {
  background-color: #b5b5c3;
  color: #000;  
  font-size: 12px;
  line-height: 20px;
  float: left;
  height: 20px;
  width: 105px;
  margin-top:     5px;
  margin-bottom:  5px;  
  margin-right:   8px;
  margin-left:    10px;  
  padding-left:   3px;
  padding-right:  3px;
  overflow: hidden;
  white-space: nowrap;
  cursor: text;
  text-align: left;
