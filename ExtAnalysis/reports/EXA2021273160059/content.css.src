.getdata-sidebar {
  position: fixed !important;
  top: 20px;
  right: 130px;
  z-index: 2147483640;
  width: 320px;
  height: 580px;

  background: #272727;
  /*border: 0.5px solid rgba(22, 22, 22, 0.6);*/
  box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.35);
  border-radius: 4px;  
  word-break: normal;
}

/** Top bar **/
.getdata-sidebar #getdata-header {
  cursor: grab;
  width: 320px;  
  height: 40px;
  background: #131313;
  /*box-shadow: 0px 1px 0px rgba(255, 255, 255, 0.05);*/
  -webkit-box-sizing: content-box !important;
  box-sizing: content-box !important;
  border-radius: 4px 4px 0px 0px;  
  float:left;  
}

.getdata-sidebar #getdata-header #help-icon {
  height: 16px;
  width: 16px;
  float: left;
  margin-left: 12px;
  margin-top: 12px;
  cursor: pointer;
}

.getdata-sidebar #getdata-header #extension-name {
  height: 9px;
  width: 72px;
  float: left;
  margin-left: 96px;
  margin-top: 16px;
  
}

.getdata-sidebar #getdata-header #extension-name img.logo-icon {
  display: table;
  pointer-events: none;
  outline: none;
}

.getdata-sidebar #getdata-header #close-icon {
  height: 16px;
  width: 16px;
  float: right;
  margin-right: 12px;
  margin-top: 12px;
  cursor: pointer;
}


/** Community recipes **/
.getdata-sidebar #datasource_recommendations_holder {
  float: right;
  height: 20px;
  width: auto; 
  margin-right: 20px;  
  margin-top: 13px;
  margin-bottom: 13px;
  cursor: pointer;
  white-space: nowrap;
  border-radius: 3px;  
  padding: 4px;
}

.getdata-sidebar #datasource_recommendations_holder:hover {
  background: #363535;
}


.getdata-sidebar #datasource_recommendations_holder #recommendations_num {
  color: #8D8D8D;
  font-family: "Open Sans";
  font-style: normal;
  font-weight: 600;
  font-size: 12px;
  line-height: 16px;
  display: flex;
  align-items: center;
  text-align: center;
  display: inline-block;
  pointer-events: none;  
}

.getdata-sidebar #datasource_recommendations_holder:hover #recommendations_num {
  color: #FFFFFF;
}

.getdata-sidebar #datasource_recommendations_holder #recommendations_icon {
  display:inline-block; 
  position: relative;
  top: 3px;
  pointer-events: none;
  background-image: url("chrome-extension://__MSG_@@extension_id__/images/recommendation-inactive.svg");   
  height: 14px;
  width: 22px;
}

.getdata-sidebar #datasource_recommendations_holder:hover #recommendations_icon {
  background-image: url("chrome-extension://__MSG_@@extension_id__/images/recommendation-active.svg");   
}

/** Footer columns counter sections **/
.getdata-sidebar #columns {
  height: 40px !important;
  max-width: 280px;
  position: absolute;
}

.getdata-sidebar #columns .column {
  cursor: pointer;
  position: relative;
