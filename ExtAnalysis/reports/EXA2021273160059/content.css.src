/* Customize the label (the container) */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label {
  display: block;
  position: relative;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;

  height: 16px;
  line-height: 16px;
  margin: 0px;

  padding-top: 12px;
  padding-bottom: 12px;
  padding-left: 44px;      
  box-sizing: content-box;  

  font-family: Open Sans;
  font-style: normal;
  font-weight: normal;
  font-size: 12px;
  align-items: center;

  color: #8D8D8D;

}

.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column:hover .recommended-label {
  /*color: #FFFFFF;    */
}

.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column.active .recommended-label {
  color: #FFFFFF;    
}

/* Hide the browser's default checkbox */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
  margin: 0px;
}

/* Create a custom checkbox */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .checkmark {
  position: absolute;
  top: 12px;
  display: block !important;
  left: 15px !important;
  height: 16px;
  width: 16px;
  background-color: #222222;
  border-radius: 2px;
  border: 1px solid #8D8D8D;
  box-sizing: border-box;
}

.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column:nth-child(odd) .checkmark {
  background-color: #2F2F2F;
}

/* On mouse-over, add a grey background color */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label:hover input ~ .checkmark {
  background-color: #ccc;
}

/* When the checkbox is checked */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label input:checked ~ .checkmark {
  background: #F03737;
}

/* Create the checkmark/indicator (hidden when not checked) */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .checkmark:after {
  content: "";
  position: absolute;
  display: none;
}

/* Show the checkmark when checked */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label input:checked ~ .checkmark:after {
  display: block;
}

/* Style the checkmark/indicator */
.getdata-sidebar.recommendations-mode #column-recommendations-listings .recommended_panel_column .recommended-label .checkmark:after {
  left: 3px;
  top: 0px;
  width: 4px;
  height: 9px;

  border: solid white;
  border-width: 0 3px 3px 0;
  box-sizing: content-box;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}



