<html>
  <head>
    <!-- Jasmine Test -->
    <link rel="stylesheet" type="text/css" href="./jasmine-1.2.0/jasmine.css">
    <script type="text/javascript" src="./jasmine-1.2.0/jasmine.js"></script>
    <script type="text/javascript" src="./jasmine-1.2.0/jasmine-async.js"></script>
    <script type="text/javascript" src="./jasmine-1.2.0/jasmine-html.js"></script>
    <script type="text/javascript" src="./helpers/console-runner.js"></script>
    <script type="text/javascript" src="./helpers/browser.js"></script>    

    <!-- Start: Handlebar fixtures -->
    <script id="fixture_column" type="text/x-handlebars-template">
      <div class='stick-holder'></div>
      <div class='col-name' contenteditable='true'>{{ col_name }}</div>
      <div class='counter-holder'>
        <span class='counter'>11</span>
      </div>
    </script>

    <script id="fixture_sidebar" type="text/x-handlebars-template">
      <div id='header'>
        {{{img "images/logo-a.png" "logo" "logos" }}}
        <div id='name'>Get Data.IO</div>
      </div>
      <div id='add_columns'>Add Something</div>
      <div id='columns'></div>
    </script>    
    <!-- End: Handlebar fixtures -->    



    <!-- dependent libraries -->
    <script type="text/javascript" src="../js/config/config.js"></script>
    <script type="text/javascript" src="../js/content/lib/jquery_1.9.1.min.js"></script>
    <script type="text/javascript" src="../js/content/lib/jquery_ui_1.10.3.js"></script>
    <script type="text/javascript" src="../js/content/lib/handlebars.js"></script>
    <script type="text/javascript" src="../js/content/lib/handlebar-helpers.js"></script>
    <script type="text/javascript" src="../js/content/lib/underscore-min.js"></script>
    <script type="text/javascript" src="../js/content/lib/backbone-min.js"></script>
    <script type="text/javascript" src="../js/content/lib/backbone-override.js"></script>



    <!-- Application Files -->
    <script type="text/javascript" src="../js/content/models/tab.js"></script>
    <script type="text/javascript" src="../js/content/models/page.js"></script>
    <script type="text/javascript" src="../js/content/models/pagination.js"></script>
    <script type="text/javascript" src="../js/content/models/column.js"></script>
    <script type="text/javascript" src="../js/content/views/selected_dom_view.js"></script>
    <script type="text/javascript" src="../js/content/views/column_view.js"></script>
    <script type="text/javascript" src="../js/content/views/pagination_view.js"></script>
    <script type="text/javascript" src="../js/content/views/sidebar_view.js"></script>
    <script type="text/javascript" src="../js/content/views/tab_view.js"></script>
    <script type="text/javascript" src="../js/content/application.js"></script>
    
      
    <!-- Spec Files -->
    <script type="text/javascript" src="js/content/application.spec.js"></script>
    <script type="text/javascript" src="js/content/views/tab_view.spec.js"></script>
    <script type="text/javascript" src="js/content/views/sidebar_view.spec.js"></script>
    <script type="text/javascript" src="js/content/views/column_view.spec.js"></script>

  </head>
  <body>

    <h1>Table</h1>
    <table>
      <tr>
        <td>row1 col1</td>
        <td>row1 col2</td>
      </tr>
      <tr>
        <td>row2 col1</td>
        <td>row2 col2</td>
      </tr>
      <tr>
        <td>row3 col1</td>
        <td>row3 col2</td>
      </tr>
    </table>

    <h1>Rooted Div</h1>
    <div>Rooted div</div>

    <h1>Nested Level1 Div</h1>
    <div>
      <div>div1</div>
      <div>div2</div>
      <div>div3</div>
    </div>

    <h1>Nested Level2 Div</h1>
    <div>
      <div>
        <div>Nested1 div1</div>
        <div>Nested1 div2</div>
      </div>
      <div>
        <div>Nested2 div1</div>
        <div>Nested2 div2</div>
      </div>
      <div>
        <div>Nested3 div1</div>
        <div>Nested3 div2</div>
      </div>
    </div>

    <h1>Spans</h1>
    <div>
      <span>Span1</span><br>
      <div>
        <span>Nested Span1</span>
      </div>

      <span>Span2</span><br>
      <div>
        <span>Nested Span2</span>
      </div>

      <span>Span3</span>
      <div>
        <span>Nested Span2</span>
      </div>
    </div>

    <h1>Adjacent Spans</h1>
    <div>
      <span>Adjacent Span1</span><br>
      <div>
        <span>Adjacent Nested Span1</span>
      </div>

      <span>Adjacent Span2</span><br>
      <div>
        <span>Adjacent Nested Span2</span>
      </div>

      <span>Adjacent Span3</span>
      <div>
        <span>Adjacent Nested Span2</span>
      </div>
    </div>

    <h1>Jagged Spans</h1>
    <div>
      <span>Span1</span>
      <span>
        <span>Adjacent Nested Span1</span>
      </span>
      <br>
      <span>Adjacent Span2</span>
      <span>
        <span>Adjacent Nested Span2</span>
      </span>
      <br>
      <span>Adjacent Span3</span>
      <span>
        <span>Adjacent Nested Span3</span>
      </span>
    </div>


    <h1>Very nested Spans</h1>
    <div>
      <span>Span1</span>
      <div>
        <div>
          <div>
            <span>super Nested Span1</span>
          </div>
        </div>
      </div>
      <br>

      <span>Adjacent Span2</span>
      <div>
        <div>
          <div>
            <span>super Nested Span2</span>
          </div>
        </div>
      </div>
      <br>

      <span>Adjacent Span3</span>
      <div>
        <div>
          <div>
            <span>super Nested Span3</span>
          </div>
        </div>
      </div>
    </div>

  </body>
  <footer>
    <script type="text/javascript">
      var console_reporter = new jasmine.ConsoleReporter()
      jasmine.getEnv().addReporter(new jasmine.TrivialReporter());
      jasmine.getEnv().addReporter(console_reporter);
      jasmine.getEnv().execute();
    </script>  
  </footer>
</html>