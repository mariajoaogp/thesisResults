{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Bark for Chrome",
  "short_name": "Bark",
  "description": "Monitor chrome web browsing for inappropriate content.",
  "version": "1.3.3",
  "permissions": [
    "tabs",
    "identity",
    "identity.email"
  ],
  "optional_permissions": [
    "management"
  ],
  "background": {
    "service_worker": "monitor.js"
  },
  "icons" : {
    "48" : "icon_48.png",
    "128" : "icon_128.png"
  },
  "manifest_version": 3
}
